		{% for d in result['data'] %}
		<div class="rightcontent">
			<p class="soo_life">{{d['name']}} </p>
				<div class="planer"> 
					<hr> 
					<ul class="all_player">
						<li class="noyeter">
							<img src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">   <!--已提交高亮-->
						</li>

						{% if d['status'] < 1 %}
							<li class="noyeter">
								<img src="/public/img/shopseek/mine_zhifu_yuan@2x.png">  <!--资料审核灰色-->
							</li>
							{% elseif (d['status'] > 0) and (d['status'] < 7 ) %}
