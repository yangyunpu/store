							{% elseif k['status'] == 9 %}
								<li class="noyet">
									<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   <!--洽谈中灰色-->
								</li>
							{% elseif k['status'] == 2 %}
								<li class="noyet">
									<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   <!--洽谈中灰色-->
								</li>
						{% else %}
							<li class="noyet">
								<img class="nohat2" src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">   <!--洽谈中亮色-->
							</li>

					{% endif %}



					{% if k['status'] < 3 %}
						<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">  <!--资料审核灰色-->
						</li>

					{% elseif k['status'] > 2 and k['status'] < 5 %}
						<li class="noyet">
								<img class="nohat2" src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">     <!--资料审核亮色-->
							</li>
						{% elseif k['status'] == 9%}
							<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   	<!--资料审核灰色-->
						</li>
						{% elseif k['status'] == 2%}
							<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   	<!--资料审核灰色-->
						</li>
						{% else %}
							<li class="noyet">
								<img class="nohat2" src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">   <!--资料审核亮色-->
							</li>

					{% endif %}


					{% if k['status'] < 5 %}
						<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   <!--签约中灰色-->
						</li>
					{% elseif k['status'] > 4 and k['status'] <7 %}
						<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">  <!--签约中亮色-->
						</li>
						{% elseif k['status'] == 9 %}
							<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   <!--签约中灰色-->
						</li>
						{% elseif k['status'] == 2%}
							<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   <!--签约中灰色-->
						</li>
						{% else %}
						<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">   <!--签约中亮色-->
						</li>

					{% endif %}

					{% if k['status'] == 10 or k['status'] == 11 %}
						<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">
						</li>
						{% elseif k['status'] == 2 or k['status'] == 9 %}
						<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">
						</li>
						{% else %}
						<li class="noyet">
							<img class="nohat2" src="/public/img/shopseek/mine_zhifu_yuan@2x.png">
						</li>
					{% endif %}
				</ul>
				<ul class="play">
					<li>已提交</li>
					<li>洽谈中</li>
					<li>资料审核</li>
					<li>签约中</li>
					<li>已返现</li>
				</ul> 
				</div>
				<ul class="cond"> 
					{% for s in k['list_log']%}
					<li>
						<div class="statusd">
							<div class="statusd2">
							 	<img class="lastdiand" src="/public/img/shopseek/2@2x.png"> 
							</div>
							<div class="rightstatusd">
							 	<p class="textd1">{{s['result']}}</p>
							 	<p class="textd1_time">{{date("Y-m-d H:i:s",s['time'])}}</p>
							</div> 
						</div>
					</li>
					{% endfor %}
				</ul> 
		</div>
		{% endfor %}
		{% endif %}	 
		<!-- 商家签约 -->
