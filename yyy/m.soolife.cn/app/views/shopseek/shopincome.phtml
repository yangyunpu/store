									<li class="noyeter">
										<img src="/public/img/shopseek/mine_zhifu_yuan@2x.png">  <!--资料审核灰色-->
									</li>
								{% else %}
									<li class="noyeter">
										<img src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">   <!--资料审核高亮-->
									</li>
								{% endif %}
						{% else %}
							<li class="noyeter">
								<img src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">   <!--资料审核高亮-->
							</li>
						{% endif %}



						{% if d['status'] <= 7  %}
							<li class="noyeter">
								<img src="/public/img/shopseek/mine_zhifu_yuan@2x.png">     <!--已返现灰色-->
							</li>
							{% elseif (d['status'] == 11) or (d['status'] == 10) %}
								{% if d['status'] == 2 %}
									<li class="noyeter">
										<img src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   <!--已返现灰色-->
									</li>
								{% elseif d['status'] == 11 %}
									<li class="noyeter">
										<img src="/public/img/shopseek/mine_zhifu_d_yuan@2x.png">    <!--已返现高亮-->
									</li>
							{% else %}
								<li class="noyeter">
									<img src="/public/img/shopseek/mine_zhifu_yuan@2x.png">   <!--已返现灰色-->
								</li>
							{% endif %}
						{% endif %}
					</ul>
					<ul class="player">
						<li>已提交</li> 
						<li>资料审核</li> 
						<li>已返现</li>
					</ul> 
				</div>

				<!-- 正在审核 -->
				<ul class="cond"> 
					{% for v in d['list_log']%}
					<li>
						<div class="statusd">
							<div class="statusd2">
							 	<img class="lastdiand" src="/public/img/shopseek/2@2x.png"> 
							</div> 
							<div class="rightstatusd">
							 	<p class="textd1">{{v['result']}}</p>
							 	<p class="textd1_time">{{date("Y-m-d H:i:s",v['time'])}}</p>
							</div> 
						</div>
					</li>
					{% endfor %}
