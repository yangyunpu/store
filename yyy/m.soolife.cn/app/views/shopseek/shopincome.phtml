				<div class="red"></div>
			</div>
			<div class="rightshop">
				<div class="shopyue">商家签约</div>
				<div class="red2"></div>
			</div>
		</div>
		<div class="codes"></div>
		<!-- 商家报备 -->
		{% if data%}
		{% for  k in data['data'] %}
